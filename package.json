{
	"name": "fmentor-stats-preview-react-styled-components",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1",
		"styled-components": "^6.0.4",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.16",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.31",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.8",
		"sass": "^1.62.1"
	},
	"scripts": {
		"sass-dev": "sass --watch src/assets/scss/main.scss src/assets/css/main.css",
		"sass-prod": "sass src/assets/scss/main.scss src/assets/css/main.css --style=compressed --no-source-map",
		"postcss": "postcss src/assets/css/main.css -o src/assets/css/main.css --use autoprefixer --no-map",
		"prettier": "prettier --write .",
		"dev": "run-s sass-dev",
		"lint": "run-s prettier",
		"build-css": "run-s lint sass-prod postcss",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
